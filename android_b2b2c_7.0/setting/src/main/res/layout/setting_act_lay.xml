<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="data"
            type="com.enation.javashop.android.middleware.model.MemberViewModel"/>
        <import type="com.enation.javashop.android.middleware.bind.DataBindingHelper"/>
        <import type="android.view.View"/>
    </data>
    <android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:bind="http://schemas.android.com/apk/res-auto"
        bind:backgroundColor="@{@color/javashop_color_white}"
        android:background="#ffffff"
        >
        <com.enation.javashop.android.lib.widget.CommonActionBar
            android:id="@+id/setting_top_bar"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            bind:auto_topbar_height="@{true}"
            app:title_text="设置"
            app:title_text_color="@color/javashop_color_black"
            app:left_image="@drawable/javashop_back_black"
            app:holder_visibility="false"
            app:background_color="@color/javashop_color_white"
            />
        <android.support.constraint.ConstraintLayout
            android:id="@+id/setting_action_userinfo"
            app:layout_constraintTop_toBottomOf="@id/setting_top_bar"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:visibility="@{ data == null ? View.GONE : View.VISIBLE }"
            app:layout_constraintDimensionRatio="h,4:1"
            >
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/setting_userface_iv"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.05"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.8"
                app:layout_constraintDimensionRatio="w,1:1"
                android:src="@mipmap/member_nologin_userface"
                app:civ_border_color="@color/umeng_socialize_grid_divider_line"
                app:civ_circle_background_color="@color/javashop_color_member_userface_nologin_bg_color"
                app:civ_border_width="1dp"
                bind:url_without_holder="@{data.face}"
                />
            <TextView
                android:id="@+id/setting_nikename_tv"
                android:layout_marginLeft="10dp"
                app:layout_constraintVertical_bias="0.3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.25"
                android:text="@{data.nikename}"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@id/setting_userface_iv"
                app:autoSizeMaxTextSize="20sp"
                app:autoSizeMinTextSize="10sp"
                app:autoSizeStepGranularity="1sp"
                app:autoSizeTextType="uniform"
                tools:ignore="MissingPrefix"
                android:textColor="@color/javashop_color_setting_nikename_color"
                android:lines="1"
                android:ellipsize="end"
                android:gravity="center_vertical"
                />
            <TextView
                android:id="@+id/setting_username_tv"
                android:layout_marginLeft="10dp"
                app:layout_constraintVertical_bias="0.7"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintWidth_default="percent"
                app:layout_constraintWidth_percent="0.5"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.2"
                android:text="@{DataBindingHelper.userNameFix( data.username )}"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toRightOf="@id/setting_userface_iv"
                android:textColor="@color/javashop_color_setting_username_color"
                android:lines="1"
                android:ellipsize="end"
                />
            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toRightOf="@id/setting_username_tv"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.3"
                app:layout_constraintDimensionRatio="w,1:1"
                android:src="@drawable/javashop_into_gray"
                app:layout_constraintHorizontal_bias="0.8"
                />
            <View
                android:id="@+id/setting_line_a"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/javashop_color_member_line_view_color"
                app:layout_constraintBottom_toBottomOf="parent"
                />
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:id="@+id/setting_action_lay"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintHeight_default="percent"
            app:layout_constraintHeight_percent="0.5"
            app:layout_constraintTop_toBottomOf="@id/setting_action_userinfo"
            >
            <View
                android:id="@+id/setting_holder_a"
                android:layout_height="0dp"
                android:layout_width="match_parent"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.05"
                android:background="@color/javashop_color_setting_holder_color"
                />
            <View
                  android:id="@+id/setting_line_b"
                  android:layout_width="match_parent"
                  android:layout_height="1dp"
                  android:background="@color/javashop_color_member_line_view_color"                  app:layout_constraintTop_toBottomOf="@id/setting_holder_a"
                />
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_receipt"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:visibility="gone"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintTop_toBottomOf="@id/setting_line_b"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:lines="1"
                    android:text="@string/javashop_setting_action_receipt"
                    android:textColor="@color/javashop_color_setting_action_color"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.5"
                    tools:ignore="MissingPrefix"
                    />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="right"
                    android:lines="1"
                    android:text="发票相关"
                    android:textColor="#ccc"
                    style="@style/auto_text_size"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintHorizontal_bias="0.9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.15"
                    tools:ignore="MissingPrefix"
                    />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintDimensionRatio="w,1:1"
                    android:src="@drawable/javashop_into_gray"
                    app:layout_constraintHorizontal_bias="0.97"
                    />
                <View
                    android:id="@+id/setting_line_c"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.95"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </android.support.constraint.ConstraintLayout>
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_privacy"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:visibility="gone"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintHeight_percent="0.15"
                app:layout_constraintTop_toBottomOf="@id/setting_action_receipt"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="MissingPrefix"
                    android:lines="1"
                    android:ellipsize="end"
                    android:text="@string/javashop_setting_action_privacy"
                    android:textColor="@color/javashop_color_setting_action_color"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="right"
                    android:lines="1"
                    android:text="隐私相关"
                    android:textColor="#ccc"
                    style="@style/auto_text_size"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintHorizontal_bias="0.9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.15"
                    tools:ignore="MissingPrefix"
                    />
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintDimensionRatio="w,1:1"
                    android:src="@drawable/javashop_into_gray"
                    app:layout_constraintHorizontal_bias="0.97"
                    />
                <View
                    android:id="@+id/setting_line_d"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.95"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </android.support.constraint.ConstraintLayout>
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_share"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintDimensionRatio="h,8:1"
                app:layout_constraintTop_toBottomOf="@id/setting_action_privacy"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="MissingPrefix"
                    android:lines="1"
                    android:ellipsize="end"
                    android:text="@string/javashop_setting_action_share"
                    android:textColor="@color/javashop_color_setting_action_color"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="right"
                    android:lines="1"
                    android:text="应用引流"
                    android:textColor="#ccc"
                    style="@style/auto_text_size"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintHorizontal_bias="0.9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.15"
                    tools:ignore="MissingPrefix"
                    />
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintDimensionRatio="w,1:1"
                    android:src="@drawable/javashop_into_gray"
                    app:layout_constraintHorizontal_bias="0.97"
                    />
                <View
                    android:id="@+id/setting_line_e"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.95"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </android.support.constraint.ConstraintLayout>
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_cache"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintDimensionRatio="h,8:1"                app:layout_constraintTop_toBottomOf="@id/setting_action_share"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:lines="1"
                    android:text="@string/javashop_setting_action_cache"
                    android:textColor="@color/javashop_color_setting_action_color"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.5"
                    tools:ignore="MissingPrefix"
                    />
                <TextView
                    android:id="@+id/cache_tv"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="right"
                    android:lines="1"
                    android:text="0KB"
                    android:textColor="#ccc"
                    style="@style/auto_text_size"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintHorizontal_bias="0.9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.15"
                    tools:ignore="MissingPrefix"
                    />
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintDimensionRatio="w,1:1"
                    android:src="@drawable/javashop_into_gray"
                    app:layout_constraintHorizontal_bias="0.97"
                    />
                <View
                    android:id="@+id/setting_line_f"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.95"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </android.support.constraint.ConstraintLayout>
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_about"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintDimensionRatio="h,8:1"
                app:layout_constraintTop_toBottomOf="@id/setting_action_cache"
                >
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.5"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="MissingPrefix"
                    android:lines="1"
                    android:ellipsize="end"
                    android:text="@string/javashop_setting_action_about"
                    android:textColor="@color/javashop_color_setting_action_color"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:ellipsize="end"
                    android:gravity="right"
                    android:lines="1"
                    android:text="相关信息"
                    android:textColor="#ccc"
                    style="@style/auto_text_size"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintHorizontal_bias="0.9"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.15"
                    tools:ignore="MissingPrefix"
                    />
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintHeight_percent="0.4"
                    app:layout_constraintDimensionRatio="w,1:1"
                    android:src="@drawable/javashop_into_gray"
                    app:layout_constraintHorizontal_bias="0.97"
                    />
                <View
                    android:id="@+id/setting_line_g"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"                    app:layout_constraintWidth_default="percent"
                    app:layout_constraintWidth_percent="0.95"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </android.support.constraint.ConstraintLayout>
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toBottomOf="@id/setting_action_about"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:background="@color/javashop_color_setting_holder_color"
                />
        </android.support.constraint.ConstraintLayout>
        <android.support.constraint.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/setting_action_lay"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@color/javashop_color_setting_holder_color"
            >
            <android.support.constraint.ConstraintLayout
                android:id="@+id/setting_action_logout"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintHeight_default="percent"
                app:layout_constraintDimensionRatio="h,7:1"
                app:layout_constraintTop_toTopOf="parent"
                android:visibility="@{ data == null ? View.GONE : View.VISIBLE }"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintVertical_bias="0.1"
                android:background="@color/javashop_color_white"
                bind:backgroundColor="@{@color/javashop_color_white}"
                >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintHeight_default="percent"
                    app:layout_constraintHeight_percent="0.5"
                    android:text="@string/javashop_setting_action_logout"
                    android:textColor="@color/javashop_color_setting_action_color"
                    android:gravity="center"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="10sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"
                    tools:ignore="MissingPrefix"
                    />

                <View
                    android:id="@+id/setting_line_h"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/javashop_color_member_line_view_color"
                    app:layout_constraintBottom_toBottomOf="parent"
