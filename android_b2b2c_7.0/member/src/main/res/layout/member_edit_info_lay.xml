            bind:layout_constraintDimensionRatio="h,4.5:1"
            bind:layout_constraintHorizontal_bias="0"
            app:layout_constraintTop_toBottomOf="@id/member_edit_info_topbar"
            app:layout_constraintWidth_percent="0.3"
            style="@style/auto_text_size"
            android:textColor="@color/javashop_color_black"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            app:autoSizeMaxTextSize="30sp"
            app:autoSizeMinTextSize="1sp"
            app:autoSizeStepGranularity="2sp"
            app:autoSizeTextType="uniform"
            tools:ignore="MissingPrefix"
            />
        <TextView
