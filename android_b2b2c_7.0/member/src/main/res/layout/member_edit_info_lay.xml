            app:layout_constraintBottom_toBottomOf="@id/member_edit_info_tel_header"
            app:layout_constraintWidth_percent="0.65"
            style="@style/auto_text_size"
            android:text="@{data.tel}"
            android:gravity="right"
            android:ellipsize="end"
            android:textColor="#c2c3c5"
            android:layout_marginRight="10dp"
            app:autoSizeMaxTextSize="30sp"
            app:autoSizeMinTextSize="1sp"
            app:autoSizeStepGranularity="2sp"
            app:autoSizeTextType="uniform"
            tools:ignore="MissingPrefix"
            />

        <View
            android:id="@+id/member_edit_info_tel_line"
            android:layout_width="0dp"
            android:layout_height="1px"
            app:layout_constraintTop_toBottomOf="@id/member_edit_info_tel_header"
            android:background="@color/javashop_color_line_gray"
            android:layout_marginTop="10dp"
            />


        <TextView
            android:id="@+id/member_edit_info_save"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/member_edit_info_tel_line"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintWidth_percent="0.8"
            app:layout_constraintDimensionRatio="h,7:1"
            style="@style/auto_text_size"
            android:textColor="@color/javashop_color_white"
            android:text="保存"
            android:background="@drawable/javashop_bg_btn_red_selector"
            android:gravity="center"
            android:padding="8dp"
            app:autoSizeMaxTextSize="30sp"
            app:autoSizeMinTextSize="1sp"
            app:autoSizeStepGranularity="2sp"
            app:autoSizeTextType="uniform"
            tools:ignore="MissingPrefix"
            />
    </android.support.constraint.ConstraintLayout>
</layout>